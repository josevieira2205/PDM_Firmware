{
    "version": 4,
    "configurations": [
        {
            /** 
             * ms-vscode.cmake-tools plugin should be installed.
             * 
             * It provides data for C/C++ plugin,
             * such as include paths, browse paths, defines, etc.
             */
            "name": "STM32",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "${default}",
            "includePath": [
                "${workspaceFolder}/**",                      // Include all subdirectories
                "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/STM32F1xx_HAL_Driver/Inc",  // HAL drivers
                "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/CMSIS/Device/ST/STM32F1xx/Include",  // CMSIS device files
                "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/CMSIS/Include" 
            ],
            "defines": [
                "STM32F1xx",
                "USE_HAL_DRIVER"
                
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",                    // Include all subdirectories
                    "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/STM32F1xx_HAL_Driver/Inc",  // HAL drivers
                    "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/CMSIS/Device/ST/STM32F1xx/Include",  // CMSIS device files
                    "C:/Users/josev/Documents/GIT/PDM_Firmware/Drivers/CMSIS/Include" 
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ]
}